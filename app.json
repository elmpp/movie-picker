{
  "expo": {
    "name": "Movie Picker",
    "slug": "movie-picker",
    "privacy": "public",
    "scheme": "moviepicker",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Simple movie browsing app built as a test for a job in NL",
    "githubUrl": "https://github.com/elmpp/movie-picker"
  }
}
